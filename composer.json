{
	"name": "raxos/terminal",
	"description": "Base classes for custom terminals with command support.",
	"license": "mit",
    "minimum-stability": "dev",
    "prefer-stable" : true,
	"type": "library",
	"authors": [
		{
			"name": "Bas Milius",
			"email": "bas@mili.us"
		}
	],
	"autoload": {
		"psr-4": {
			"Raxos\\Terminal\\": "src/Raxos/Terminal"
		}
	},
	"config": {
		"platform": {
			"php": "8.4"
		}
	},
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "symlink": true
        }
    ],
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*"
    },
	"require": {
        "ext-mbstring" : "*",
		"jetbrains/phpstorm-attributes": "^1.2",
        "league/climate": "^3.10",
        "nunomaduro/collision": "^8.5",
        "raxos/foundation": "*"
	}
}
